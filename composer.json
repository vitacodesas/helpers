{
    "name": "vitacode/helpers",
    "description": "A simple helpers package to help you with your Laravel projects",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Vitacode\\Helpers\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vitacode\\Helpers\\HelpersServiceProvider"
            ]
        }
    },
    "archive": {
        "exclude": [
            "/tests",
            "/docs",
            "/.git",
            "/.github",
            "/.env",
            "composer.lock",
            "*.log",
            "*.md",
            "/testbench"
        ]
    },
    "require": {}
}
